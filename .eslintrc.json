{
  "env": {
    "browser": true,
    "commonjs": false,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": 0,
    "import/no-commonjs": "error",
    "import/extensions": ["error", "ignorePackages", { "js": 1 }]
  }
}
